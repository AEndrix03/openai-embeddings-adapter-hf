ADAPTER_MODEL_ID=jinaai/jina-embeddings-v2-base-code
ADAPTER_MODEL_DEVICE=auto
ADAPTER_MODEL_DTYPE=auto
ADAPTER_MODEL_TRUST_REMOTE_CODE=true
ADAPTER_MODEL_STRICT_LOADING=true
CUDA_TORCH_BASE_IMAGE=pytorch/pytorch:2.4.1-cuda12.1-cudnn9-runtime
ROCM6_TORCH_BASE_IMAGE=rocm/pytorch:rocm6.4_ubuntu24.04_py3.12_pytorch_release_2.4.1
HIP_VISIBLE_DEVICES=0
HSA_OVERRIDE_GFX_VERSION=
ADAPTER_LOAD_MODEL_ON_STARTUP=false
ADAPTER_EMBEDDING_DIM=768
ADAPTER_DEFAULT_NORMALIZE=true
ADAPTER_CACHE_ENABLED=true
