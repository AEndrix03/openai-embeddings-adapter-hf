ADAPTER_MAX_BATCH_SIZE=128
ADAPTER_MAX_CHARS_PER_ITEM=10000
ADAPTER_MAX_TOTAL_CHARS=100000
ADAPTER_MAX_LENGTH_TOKENS=512
ADAPTER_AUTH_MODE=none
